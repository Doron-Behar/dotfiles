## Check out /usr/share/doc/ncmpcpp/bindings for more explenations and examples

def_key "k"
	scroll_up
def_key "j"
	scroll_down
def_key "d"
	delete_playlist_items
def_key "d"
	delete_browser_items
def_key "d"
	delete_stored_playlist
def_key "ctrl-h"
	dummy
def_key "ctrl-l"
	dummy
def_key "insert"
	select_item
	push_character "down"
