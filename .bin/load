#!/bin/zsh
. /etc/profile
if [ -f "$HOME/.profile" ]; then
	. "$HOME/.profile"
fi
if [ -f "$HOME/.zshenv" ]; then
	. "$HOME/.zshenv"
fi
if [ -f "$HOME/.zlogin" ]; then
	. "$HOME/.zlogin"
fi
if [ -f "$HOME/.zprofile" ]; then
	. "$HOME/.zprofile"
fi
if [ "$1" = "bash" ]; then
	. ~/.bashrc
elif [ "$1" = "zsh" ]; then
	. ~/.zshrc
fi
